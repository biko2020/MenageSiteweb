mixin footer(footer)

  footer
// Page Footer
footer.section.section-fluid.footer-classic
  .container-fluid
    .row.row-30.justify-content-center
      // Cadre Adresse
      .col-md-10.col-lg-12.col-xl-4.wow.fadeInRight
        .box-footer.box-footer-small
          .footer-brand
            a(href='index.html')
              img(src='images/logo-3roses-1024x279-trans.png', alt='', width='229', height='43')
          p.text-width-medium
            | #{PROPOS}
          .contact-classic
            .contact-classic-item
              .unit.align-items-center
                .unit-left
                  h6.contact-classic-title Si&egrave;ge Social
                .unit-body.contact-classic-link
                a(href='#') 344 Boul Goyer St-Eustache QC J7P 5K1- Qu&eacute;bec-Canada 
            .contact-classic-item
              .unit.align-items-center
                .unit-left
                  h6.contact-classic-title T&eacute;l&eacute;phone
                .unit-body.contact-classic-link
                  a(href='tel:#') +1 (514) 463-9734
            .contact-classic-item
              .unit.align-items-center
                .unit-left
                  h6.contact-classic-title E-mails
                .unit-body.contact-classic-link
                  a(href='mailto:#') aitoufkir.rachid@yahoo.net
                  a(href='mailto:#') admin@3rosesmenage.net
                  a(href='mailto:#') info@rosesmenage.com
          ul.list-inline.list-inline-sm.footer-social-list
            li
              a.icon.fa.fa-facebook(href='#')
            li
              a.icon.fa.fa-twitter(href='#')
            li
              a.icon.fa.fa-google-plus(href='#')
            li
              a.icon.fa.fa-instagram(href='#')
      // Cadre Contact
      .col-md-10.col-lg-6.col-xl-4.wow.fadeInRight(data-wow-delay='.1s')
        .box-footer
          h3.font-weight-normal CONTACTEZ-NOUS 
          form.rd-form.rd-mailform(data-form-output='form-output-global', data-form-type='contact', method='post', action='bat/rd-mailform.php')
            .form-wrap
              input#contact-name-6.form-input(type='text', name='name', data-constraints='@Required')
              label.form-label(for='contact-name-6') Nom
            .form-wrap
              input#contact-email-6.form-input(type='email', name='email', data-constraints='@Email @Required')
              label.form-label(for='contact-email-6') E-mail
            .form-wrap
              label.form-label(for='contact-message-6') Message
              textarea#contact-message-6.form-input(name='message', data-constraints='@Required')
            button.button.button-block.button-ujarak.button-secondary(type='submit') Envoyer
      // Cadre Nos Services
      .col-md-10.col-lg-6.col-xl-4.wow.fadeInRight(data-wow-delay='.2s')
        .box-footer
          h3.font-weight-normal Nos Services
          ul.footer-list-category
            li.heading-5
              a(href='#')
                | M&Eacute;NAGE R&Eacute;SIDENTIEL
                span
            li.heading-5
              a(href='#')
                | NETTOYAGE COMMERCIAL
                span
            li.heading-5
              a(href='#')
                | NETTOYAGE INDUSTRIEL
                span
  // CopyRight
  .container.footer-bottom-panel.wow.fadeInUp
    p.rights
      span &copy;&nbsp;
      span.copyright-year
      span Creator
      | . All rights reserved. 
      span
        | D&eacute;velopp&eacute;&nbsp;par&nbsp;
        a(href='#') Brahim Ait Oufkir
